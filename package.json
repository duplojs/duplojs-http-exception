{
  "name": "@duplojs/http-exception",
  "version": "0.0.0",
  "author": "mathcovax",
  "description": "",
  "repository": "https://github.com/duplojs/duplojs-http-exception",
  "license": "ISC",
  "keywords": [],
  "typesVersions": {
    "*": {
      "global": [
        "./types/scripts/global.d.ts"
      ],
      "*": [
        "./types/scripts/index.d.ts"
      ]
    }
  },
  "exports": {
    ".": {
      "import": "./dist/httpException.mjs",
      "require": "./dist/httpException.cjs"
    },
	"./global": {
	  "import": "./dist/httpExceptionGlobal.mjs",
      "require": "./dist/httpExceptionGlobal.cjs"
	}
  },
  "files": [
    "dist",
    "types"
  ],
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "rollup --config && npx tsc -p tsconfig.build.json",
	"test:e2e": "node --require sucrase/register test",
	"test:types": "tsc",
	"generate:type": "node --require sucrase/register scripts/generateType.ts"
  },
  "dependencies": {
    "@duplojs/duplojs": ">=1.1.8 <2.0.0",
    "@duplojs/editor-tools": ">=1.0.0"
  },
  "devDependencies": {
    "@duplojs/worker-testing": "^0.2.6",
    "@rollup/plugin-json": "^6.0.1",
    "@types/node": "^18.15.11",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "autocannon": "^7.11.0",
    "eslint": "^8.45.0",
    "rollup": "^3.26.2",
    "rollup-plugin-dts": "^5.3.0",
    "rollup-plugin-esbuild": "^5.0.0",
    "sucrase": "^3.34.0",
    "typescript": "^5.0.2"
  }
}
